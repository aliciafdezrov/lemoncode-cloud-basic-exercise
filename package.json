{
  "name": "R&M game",
  "version": "1.0.0",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "webpack serve --config ./config/webpack/dev.js --open",
    "build": "run-p -l type-check build:prod",
    "clean": "rimraf dist",
    "build:prod": "npm run clean && webpack --config ./config/webpack/prod.js",
    "analyze": "npm run clean && npm run type-check && webpack --config ./config/webpack/analyze.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "jest -c ./config/test/jest.js --verbose",
    "test:watch": "npm run test -- --watchAll -i"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "babel-loader": "^8.2.2",
    "css-loader": "5.0.1",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "1.37.0",
    "sass-loader": "12.1.0",
    "style-loader": "3.2.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.3.5",
    "webpack": "^5.46.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "7.2.6",
    "react-router-dom": "^6.0.2",
    "regenerator-runtime": "0.13.9"
  }
}
